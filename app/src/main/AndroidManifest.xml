<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eugeniojava.compufix">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CompuFix">
        <activity
            android:name=".TypeFormActivity"
            android:exported="true"
            android:parentActivityName=".TypesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TypesActivity" />
        </activity>
        <activity
            android:name=".TypesActivity"
            android:exported="true"
            android:parentActivityName=".ComputersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ComputersActivity" />
        </activity>
        <activity
            android:name=".AboutActivity"
            android:exported="true"
            android:parentActivityName=".ComputersActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ComputersActivity" />
        </activity>
        <activity android:name=".ComputersActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ComputerFormActivity" />
    </application>
</manifest>
